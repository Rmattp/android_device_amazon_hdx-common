service atfwd /system/vendor/bin/ATFWD-daemon
    class late_start
    user system
    group system radio

service ims_regmanager /system/vendor/bin/exe-ims-regmanagerprocessnative
    class late_start
    group net_bt_admin inet radio wifi
    disabled

on property:persist.ims.regmanager.mode=1
